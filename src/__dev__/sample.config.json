{
    "$schema": "https://raw.githubusercontent.com/jsonresume/resume-schema/v1.0.0/schema.json",
    "meta": {
        "theme-stackoverflowed": {
            "locale": "en-US",
            "format": {
                "date": "MMM yyyy",
                "location": [
                    "{{city}}, {{region}}",
                    "{{city}}, {{countryNameAlias}}",
                    "{{city}}, {{countryNameOfficial}}",
                    "{{city}}, {{countryCode}}",
                    "{{city}}"
                ]
            },
            "intro": {
                "avatar": {
                    "show": true,
                    "align": "right"
                }
            },
            "section": {
                "skills": {
                    "break": false,
                    "hidden": false,
                    "order": 100
                },
                "work": {
                    "break": false,
                    "hidden": false,
                    "order": 200
                },
                "volunteer": {
                    "break": false,
                    "hidden": false,
                    "order": 300
                },
                "projects": {
                    "break": false,
                    "hidden": false,
                    "order": 400
                },
                "education": {
                    "break": false,
                    "hidden": false,
                    "order": 500
                },
                "awards": {
                    "break": false,
                    "hidden": false,
                    "order": 600
                },
                "publications": {
                    "break": false,
                    "hidden": false,
                    "order": 700
                },
                "languages": {
                    "break": false,
                    "hidden": false,
                    "order": 800
                },
                "interests": {
                    "break": false,
                    "hidden": false,
                    "order": 900
                },
                "profiles": {
                    "break": false,
                    "hidden": false,
                    "order": 1000
                },
                "references": {
                    "break": false,
                    "hidden": false,
                    "order": 1100
                }
            }
        }
    }
}
