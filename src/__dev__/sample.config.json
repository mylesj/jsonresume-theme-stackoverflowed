{
    "$schema": "https://raw.githubusercontent.com/jsonresume/resume-schema/v1.0.0/schema.json",
    "meta": {
        "theme-stackoverflowed": {
            "locale": "en-US",
            "format": {
                "date": "MMM yyyy",
                "location": [
                    "{{city}}, {{region}}",
                    "{{city}}, {{countryNameAlias}}",
                    "{{city}}, {{countryNameOfficial}}",
                    "{{city}}, {{countryCode}}",
                    "{{city}}"
                ]
            },
            "intro": {
                "avatar": {
                    "show": true,
                    "align": "right"
                }
            },
            "section": {
                "skills": {
                    "break": false,
                    "hidden": false,
                    "order": 1
                },
                "work": {
                    "break": false,
                    "hidden": false,
                    "order": 2
                },
                "volunteer": {
                    "break": false,
                    "hidden": false,
                    "order": 3
                },
                "projects": {
                    "break": false,
                    "hidden": false,
                    "order": 4
                },
                "education": {
                    "break": false,
                    "hidden": false,
                    "order": 5
                },
                "awards": {
                    "break": false,
                    "hidden": false,
                    "order": 6
                },
                "publications": {
                    "break": false,
                    "hidden": false,
                    "order": 7
                },
                "languages": {
                    "break": false,
                    "hidden": false,
                    "order": 8
                },
                "interests": {
                    "break": false,
                    "hidden": false,
                    "order": 9
                },
                "profiles": {
                    "break": false,
                    "hidden": false,
                    "order": 10
                },
                "references": {
                    "break": false,
                    "hidden": false,
                    "order": 11
                }
            }
        }
    }
}
