{
    "$schema": "https://raw.githubusercontent.com/jsonresume/resume-schema/v1.0.0/schema.json",
    "meta": {
        "theme-stackoverflowed": {
            "locale": "en-US",
            "format": {
                "date": "MMM yyyy",
                "location": [
                    "{{city}}, {{region}}",
                    "{{city}}, {{countryNameAlias}}",
                    "{{city}}, {{countryNameOfficial}}",
                    "{{city}}, {{countryCode}}",
                    "{{city}}"
                ]
            },
            "intro": {
                "avatar": {
                    "show": true,
                    "align": "right"
                }
            }
        }
    }
}
