{
    "name": "jsonresume-theme-stackoverflow-jobs",
    "version": "0.0.1",
    "description": "A json-resume theme inspired by the Stack Overflow Jobs resume format.",
    "author": "mylesj <hi@mylesj.dev>",
    "license": "MIT",
    "repository": "github:mylesj/jsonresume-theme-stackoverflow-jobs",
    "bugs": "https://github.com/mylesj/jsonresume-theme-stackoverflow-jobs/issues",
    "keywords": [
        "json",
        "resume",
        "jsonresume",
        "theme",
        "stack",
        "overflow",
        "stackoverflow"
    ],
    "main": "dist/render.js",
    "module": "dist/render.mjs",
    "scripts": {
        "prepare": "husky install",
        "prebuild": "rimraf dist",
        "build": "cross-env NODE_ENV=production rollup --config rollup.config.ts --configPlugin typescript",
        "dev": "cross-env NODE_ENV=development vite",
        "test": "cross-env TZ=UTC jest --config jest.config.json",
        "lint": "eslint src --max-warnings=0"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "date-fns": "^2.28.0",
        "i18n-iso-countries": "^7.4.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@kurone-kito/jsonresume-types": "^0.2.5",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-typescript": "^8.3.1",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@types/jest": "^27.4.1",
        "@types/react": "^18.0.3",
        "@types/react-dom": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "@vitejs/plugin-react": "^1.3.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.13.0",
        "eslint-plugin-react": "^7.29.4",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.7",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.70.1",
        "ts-jest": "^27.1.4",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3",
        "vite": "^2.9.5"
    }
}
